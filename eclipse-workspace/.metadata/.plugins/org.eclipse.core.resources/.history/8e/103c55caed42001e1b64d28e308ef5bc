package com.db.dto;
import java.sql.Date;

public class BoardDto { //Data Transfer Object 데이터 전송 save container
	private int bd_no;
	private String bd_name;
	private String bd_title;
	private String bd_content;
	private Date bd_date;
	
	public BoardDto() {	//default constructor - no -argument
		super();	
	}
	
	public BoardDto(int bd_no, String bd_name, String bd_title, String bd_content, Date bd_date) {//onstructor that accepts parameters to initialize the instance variables.
		super();
		this.bd_no = bd_no;
		this.bd_name = bd_name;
		this.bd_title = bd_title;
		this.bd_content = bd_content;
		this.bd_date = bd_date;
	}
	
	public int getBd_no() {	//bd_no search
		return bd_no;
	}
	
	public void setBd_no(int bd_no) { //bd_no sett
		this.bd_no = bd_no;
	}
}


